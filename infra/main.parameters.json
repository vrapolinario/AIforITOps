{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "openAiLocation": {
      "value": "${AZURE_OPENAI_LOCATION=westus}"
    },
    "managedIdentityName": {
      "value": "${AZURE_MANAGED_IDENTITY_NAME=}"
    },
    "containerRegistryName": {
      "value": "${AZURE_CONTAINER_REGISTRY_NAME=}"
    },
    "aksClusterName": {
      "value": "${AZURE_AKS_CLUSTER_NAME=}"
    },
    "cosmosDbAccountName": {
      "value": "${AZURE_COSMOSDB_ACCOUNT_NAME=}"
    },
    "serviceBusNamespaceName": {
      "value": "${AZURE_SERVICEBUS_NAMESPACE=}"
    },
    "keyVaultName": {
      "value": "${AZURE_KEY_VAULT_NAME=}"
    },
    "openAiResourceName": {
      "value": "${AZURE_OPENAI_RESOURCE_NAME=}"
    },
    "aksKubernetesVersion": {
      "value": "${AZURE_AKS_KUBERNETES_VERSION=}"
    },
    "aksNodePoolVmSize": {
      "value": "${AZURE_AKS_NODE_POOL_VM_SIZE=Standard_D2s_v3}"
    },
    "aksSystemNodeCount": {
      "value": "${AZURE_AKS_SYSTEM_NODE_COUNT=2}"
    },
    "aksUserNodeCount": {
      "value": "${AZURE_AKS_USER_NODE_COUNT=2}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    }
  }
}
